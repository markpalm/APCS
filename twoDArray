package array;

public class twoDArray {
	public static void main(String[] args) {
		int[][] myClass = new int[10][10];
		int counter = 1;
		for(int i= 0; i <=9; i++) {
			for(int j = 0; j<= 9; j ++) {
				myClass[i][j] = counter;
				counter++;
			}
			
		}
		for (int i =0; i <= 9; i++) {
			for (int j = 0; j<= 9; j++){
				System.out.print(myClass[i][j] + " ");
			}
			System.out.println("");
		}
	}
	
}
